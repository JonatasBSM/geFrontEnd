import type {BaseResponse} from "~/actions";
import type {#upperName#} from "~/actions/#name#/index";

export interface update#upperName#Form {
    //
}

export async function update(form: update#upperName#Form) {

    const response = await usePut('/api/#name#', {
        body: form
    }, true);

    return response.data.value as BaseResponse<#upperName#>;

}